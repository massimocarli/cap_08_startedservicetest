<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="uk.co.massimocarli.startedservicetest">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <service android:name=".CounterService" android:exported="false"
             android:description="@string/service_description"/>
    <service android:name=".MultiCounterService" android:exported="false"
             android:description="@string/multi_service_description"/>
    <service android:name=".CounterIntentService" android:exported="false"
             android:description="@string/intent_service_description"/>
  </application>

</manifest>